model Confinus_DOM_Categories {
  id          Int       @id @default(autoincrement())
  companyId   Int
  company     Company   @relation(fields: [companyId], references: [id])
  name        String

  confinedSpaces Confinus_ConfinedSpace[] // Relacionamento com a tabela ConfinusConfinedSpace

  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @default(now()) @updatedAt
  inactiveAt  DateTime?
}
