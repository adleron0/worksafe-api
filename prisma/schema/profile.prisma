// Tabela Profile
model Profile {
  id        Int    @id @default(autoincrement())
  name      String @unique @db.VarChar(100)
  companyId Int?

  users       User[]
  permissions ProfilePermission[] // Relacionamento com ProfilePermission

  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @default(now()) @updatedAt
  inactiveAt DateTime?
}
