// Tabela Profile
model Profile {
  id       Int       @id @default(autoincrement())
  name     String    @db.VarChar(100) @unique
  companyId Int?

  users    User[]
  permissions ProfilePermission[]  // Relacionamento com ProfilePermission

  createdAt DateTime  @default(now())
  updatedAt DateTime  @default(now()) @updatedAt
  inactiveAt DateTime?
}
